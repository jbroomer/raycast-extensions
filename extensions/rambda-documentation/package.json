{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "rambda-documentation",
  "title": "Rambda Documentation",
  "description": "Offers quick and filterable Rambda documentation results.",
  "icon": "command-icon.png",
  "author": "jbroomer",
  "categories": [
    "Documentation",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Rambda Documentation",
      "description": "Shows any Rambda functions matching the input",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.47.3",
    "@raycast/utils": "^1.4.8",
    "puppeteer": "^19.7.2",
    "turndown": "^7.1.1"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@types/turndown": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish",
    "rambdaDump": "ts-node scripts/rambdaDump.ts"
  }
}
